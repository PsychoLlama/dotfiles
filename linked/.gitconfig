[alias]
  a = add -A
  d = diff
  l = log --pretty=oneline --abbrev-commit
  r = reset
  p = push --set-upstream origin
  s = stash
  pl = pull --prune origin
  amend = commit --amend
  ls = ls-files
[user]
  email = Jesse_Gibson@me.com
  name = Jesse Gibson
[core]
  editor = nvim
[push]
  followTags = true
  default = current
[merge]
  tool = vimdiff
[mergetool "vimdiff"]
  cmd = nvim -d "$MERGED" "$LOCAL" "$BASE" "$REMOTE" -c 'wincmd J'
